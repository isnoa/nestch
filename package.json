{
  "name": "nestch",
  "version": "1.2.1",
  "private": false,
  "description": "A CLI tool for renaming NestJS resource names easily.",
  "main": "bin/cli.js",
  "scripts": {
    "preinstall": "npm run format",
    "build": "tsc && node scripts/create-bin.js && node scripts/prepare-package.js && copy readme.md dist && copy LICENSE dist",
    "format": "prettier --write 'src/**/*.{ts,js,json}'",
    "start": "node dist/bin/cli.js",
    "dev": "ts-node dist/bin/cli.ts",
    "cli": "npm install && npm run build && cd dist && npm publish",
    "local": "npm install && npm run build && cd dist && npm link"
  },
  "dependencies": {
    "commander": "^14.0.0",
    "prompts": "^2.4.2"
  },
  "devDependencies": {
    "@types/node": "^22.15.29",
    "@types/prompts": "^2.4.9",
    "typescript": "^5.8.3",
    "prettier": "^3.5.3"
  },
  "keywords": [
    "nestjs",
    "nest",
    "nestjs-cli",
    "cli",
    "change",
    "name",
    "rename",
    "resource",
    "refactor",
    "generator",
    "module",
    "controller",
    "service",
    "entity",
    "dto",
    "guard",
    "interceptor",
    "pipe",
    "filter",
    "resolver",
    "repository",
    "gateway",
    "subscriber",
    "middleware",
    "project",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/isnoa/nestch.git"
  },
  "contributors": [
    {
      "name": "isnoa",
      "email": "isnoa3166@gmail.com",
      "url": "https://github.com/isnoa"
    }
  ],
  "author": "isnoa",
  "license": "MIT",
  "bin": {
    "nestch": "bin/cli.js"
  },
  "files": [
    "dist",
    "package.json",
    "readme.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=20.0.0"
  }
}
